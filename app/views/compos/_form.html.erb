<%= simple_form_for(@compo) do |f| %>
	<%= f.association :game %>
	<%= f.input :slots %>
	<%= f.input :group_size %>
	<%= f.input :date_time, :as => :date_picker %>
	<%= f.input :description %>
	<%= f.input :bbracket_type %>
	<%= f.input :featured %>
	<%= f.input :participation_badge %>
	<%= f.input :winning_badge %>
	<%= f.button :submit %>
<% end %>

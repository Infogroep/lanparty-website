<% buttons    ||= buttons.nil? %>
<% compo      ||= compo.nil? %>
<% time       ||= time.nil? %>

<tr>
	<td><%= link_to team.name, team %></td>
	<% if compo && team.compo && team.compo.game %>
		<td><%= team.compo.game.name %></td>
	<% else %>
		<td>UNKNOWN COMPO</td>
	<% end %>
	<% if time %>
	<% end %>
	<% if buttons %>
		<td><%= standard_edit_buttons(team) %></td>
		<% end %>
</tr>
